{
  "name": "@benwis/femark-napi",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "femark-napi",
    "triples": {
      "additional": [
        "aarch64-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.9.0",
    "ava": "^4.2.0"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "version": "napi version"
  },
  "repository": "https://github.com/benwis/femark-napi",
  "description": "A blazing fast Markdown to HTML compiler with support for syntax highlighting. Created using Rust's pulldown-cmark and syntect crates, compiled for use with the Node FFI",
  "optionalDependencies": {
    "@benwis/femark-napi-win32-x64-msvc": "0.0.0",
    "@benwis/femark-napi-darwin-x64": "0.0.0",
    "@benwis/femark-napi-linux-x64-gnu": "0.0.0"
  }
}